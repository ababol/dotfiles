"*":
  editor:
    invisibles: {}
    fontSize: 12
    lineHeight: 1.4
    softWrap: true
    fontFamily: "Hack"
  core:
    ignoredNames: [
      ".bundle"
      ".git"
      "log"
      "repositories"
      "tmp"
      "vendor"
    ]
    hideGitIgnoredFiles: true
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
    ]
    themes: [
      "one-dark-ui"
      "material-monokai-syntax"
    ]
  "spell-check":
    grammars: [
      "text.plain"
      "source.gfm"
      "text.git-commit"
    ]
  "tree-view":
    hideIgnoredNames: true
  "atom-material-ui":
    slimScrollbar: true
    tabSize: "Small"
    rippleAccentColor: true
    panelContrast: true
    compactTreeView: true
    altCmdPalette: false
    tabs:
      tabMinWidth: true
      tabSize: "Small"
    treeView:
      compactTreeView: true
    ui: {}
    fonts: {}
    panels:
      panelContrast: true
  welcome:
    showOnStartup: false
  "release-notes":
    viewedVersion: "0.89.0"
  react: {}
  jsformat:
    format_on_save: false
  "tabs-to-spaces": {}
  "one-dark-ui":
    layoutMode: "Compact"
  "autocomplete-python":
    useSnippets: "all"
  "python-jedi": {}
